/* pages/teacher/extract/page2/page2.wxss */
.Section{
  font-size: 12px;
  margin: 10rpx 0rpx;
  width: 100%;
  height: 180rpx;
  line-height: 60rpx;
  border: solid 1px darkgray;
}
.table {
  margin: 30rpx 10rpx;
  background-color: #fff;
  border: 1px solid darkgray;
  font-size: 12px;
}
.tr {
  display: flex;
  width: 100%;
  height: 3rem;
}
.td,.th {
  display: flex;
  justify-content: center;  /*水平居中 */
  align-items: center; /*垂直居中*/
  width: 100%;
  border: 1px solid darkgray;
  height: 3rem;
}
.bg-w{
  background: #f2f2f2;
}

.but{
  margin:0rpx 10rpx;
  display: flex;
  justify-content: center;  /*水平居中 */
  align-items: center; /*垂直居中*/
}
.reset,.submit{
  font-weight: normal;
  border: solid 1px darkgray;
}
